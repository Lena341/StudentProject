<Window x:Class="StdProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StdProject"
        mc:Ignorable="d"
        Title="Student Application" Height="450" Width="800" Closing="Window_Closing">
    <Grid Background="AliceBlue">
        <Label x:Name="welcome" Content="Welcome" HorizontalAlignment="Left" Margin="309,62,0,0" VerticalAlignment="Top" FontSize="36" Foreground="Black"/>
        <Label Content="To start the application press Start" HorizontalAlignment="Left" Margin="262,169,0,0" VerticalAlignment="Top" FontSize="16"/>
        <Button Background="White" x:Name="startButton" Content="Start" HorizontalAlignment="Left" VerticalAlignment="Top" Width="89" Margin="347,288,0,0" Height="37" Click="startButton_Click" IsEnabled="True"/>
        <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
